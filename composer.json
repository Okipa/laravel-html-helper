{
    "name": "okipa/laravel-html-helper",
    "description": "Useful helpers to dynamically generate clean HTML with Laravel.",
    "type": "library",
    "keywords": [
        "okipa",
        "package",
        "laravel",
        "php",
        "html",
        "helper",
        "generate",
        "generator",
        "generation"
    ],
    "homepage": "https://github.com/Okipa/laravel-html-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0||^6.0||^7.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.4.3||^0.5",
        "orchestra/testbench": "~3.8.0||^4.0||^5.0",
        "phpmd/phpmd": "^2.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelHtmlHelper\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelHtmlHelper\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd src text phpmd.xml",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
